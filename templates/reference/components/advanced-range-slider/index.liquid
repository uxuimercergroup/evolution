{% layout "reference/layout" %}
{% block title %}{{ model.title }} : {{ global.site_title }}{% endblock %}
{% block content %}

<!-- PAGE INTRO - BEGINS -->
	<section class="row">

	<!-- PAGE TITLE AND DESCRIPTION - BEGINS -->
		<div class="small-12 columns">
			<h1>{{ model.title }}</h1>
			<p>Version: {{ model.version_number }}</p>
			<p>
				Lorem ipsum sed turpis nibh libero nisi integer arcu mattis quam aliquet aliquet. Viverra aliquam felis lacus mus auctor nisl eu laoreet in. Ipsum sit dolor aliquet nunc arcu consequat orci eu sociosqu in non.
			</p>
		</div>
	<!-- PAGE TITLE AND DESCRIPTION - ENDS -->

	<!-- PAGE MAGELLAN NAV - BEGINS -->
		<div class="small-12 columns">
			<div class="evo-magellan" data-magellan-expedition="fixed">
				<dl class="sub-nav">
					<dd data-magellan-arrival="example-a"><a href="#example-a">Example A</a></dd>
					<dd data-magellan-arrival="example-b"><a href="#example-b">Example B</a></dd>
					<dd data-magellan-arrival="advanced"><a href="#advanced">Advanced</a></dd>
				</dl>
			</div>
		</div>
	<!-- PAGE MAGELLAN NAV - ENDS -->

	</section>
<!-- PAGE INTRO - ENDS -->

<!-- EXAMPLE - BEGINS -->
	<section class="row">
		<div class="small-12 columns">
			<hr />

		<!-- EXAMPLE A - BEGINS -->
			<h3 data-magellan-destination="example-a">Example A</h3>
			<a name="example-a"></a>
			<p>
				Lorem ipsum sed turpis nibh libero nisi integer arcu mattis <code>quam aliquet</code> aliquet. Viverra aliquam felis lacus mus auctor nisl eu laoreet in.
			</p>

			<!-- ANNOTATIONS BOX - BEGINS -->
				<div class="panel evo-box annotations">
					<div class="evo-box-bdy">
						<p class="evo-no-margin-bottom">Note: Lorem ipsum sed turpis nibh libero.</p>
					</div>
				</div>
			<!-- ANNOTATIONS BOX - ENDS -->

			<!-- EXAMPLE DEMO - BEGINS -->
				<section class="row">

				<!-- EXAMPLE CODE - BEGINS -->
					<div class="medium-6 columns">

					<!-- EXAMPLE HTML - BEGINS -->
						<h6>HTML</h6>

						{{ "advanced-range-slider.css" | asset_url | stylesheet_tag }}

						{{ global.reference-get-code-start-tag }}{% include "/reference/components/advanced-range-slider/advanced-range-slider" %}{{ global.reference-get-code-end-tag }}
					<!-- EXAMPLE HTML - ENDS -->

					<!-- EXAMPLE JS - BEGINS -->
						<h6>Javascript</h6>
						{{ global.reference-get-code-start-tag }}{% include "/reference/components/advanced-range-slider/advanced-range-slider-js" %}{{ global.reference-get-code-end-tag }}
					<!-- EXAMPLE JS - ENDS -->

					<!-- VIEW CODE LINKS - BEGINS -->
						<ul>
							<li><a href="advanced-range-slider.css">View CSS</a></li>
							<li><a href="advanced-range-slider.js">View JS</a></li>
						</ul>
					<!-- VIEW CODE LINKS - ENDS -->

					</div>
				<!-- EXAMPLE CODE - ENDS -->

				<!-- EXAMPLE RENDERED CODE - BEGINS -->
					<div class="medium-6 columns">
						<h6>Rendered HTML</h6>
						{% include "/reference/components/advanced-range-slider/advanced-range-slider" %}
					</div>
				<!-- EXAMPLE RENDERED CODE - ENDS -->

				</section>
			<!-- EXAMPLE DEMO - ENDS -->

			<hr />
		<!-- EXAMPLE A - ENDS -->

		<!-- EXAMPLE B - BEGINS -->
			<h3 data-magellan-destination="example-b">Example B</h3>
			<a name="example-b"></a>
			<p>
				Lorem ipsum sed turpis nibh libero nisi integer arcu mattis <code>quam aliquet</code> aliquet. Viverra aliquam felis lacus mus auctor nisl eu laoreet in.
			</p>

			<!-- ANNOTATIONS BOX - BEGINS -->
				<div class="panel evo-box annotations">
					<div class="evo-box-bdy">
						<p class="evo-no-margin-bottom">Note: Lorem ipsum sed turpis nibh libero.</p>
					</div>
				</div>
			<!-- ANNOTATIONS BOX - ENDS -->

			<!-- EXAMPLE DEMO - BEGINS -->
				<section class="row">

				<!-- EXAMPLE CODE - BEGINS -->
					<div class="medium-6 columns">

					<!-- EXAMPLE HTML - BEGINS -->
						<h6>HTML</h6>

						{{ "advanced-range-slider.css" | asset_url | stylesheet_tag }}

						{{ global.reference-get-code-start-tag }}{% include "/reference/components/advanced-range-slider/advanced-range-slider" %}{{ global.reference-get-code-end-tag }}
					<!-- EXAMPLE HTML - ENDS -->

					<!-- EXAMPLE JS - BEGINS -->
						<h6>Javascript</h6>
						{{ global.reference-get-code-start-tag }}{% include "/reference/components/advanced-range-slider/advanced-range-slider-js" %}{{ global.reference-get-code-end-tag }}
					<!-- EXAMPLE JS - ENDS -->

					<!-- VIEW CODE LINKS - BEGINS -->
						<ul>
							<li><a href="advanced-range-slider.css">View CSS</a></li>
							<li><a href="advanced-range-slider.js">View JS</a></li>
						</ul>
					<!-- VIEW CODE LINKS - ENDS -->

					</div>
				<!-- EXAMPLE CODE - ENDS -->

				<!-- EXAMPLE RENDERED CODE - BEGINS -->
					<div class="medium-6 columns">
						<h6>Rendered HTML</h6>
						{% include "/reference/components/advanced-range-slider/advanced-range-slider" %}
					</div>
				<!-- EXAMPLE RENDERED CODE - ENDS -->

				</section>
			<!-- EXAMPLE DEMO - ENDS -->

			<hr />
		<!-- EXAMPLE B - ENDS -->

		<!-- ADVANCED - BEGINS -->
			<section class="row">
				<div class="small-12 columns">

					<h3 data-magellan-destination="advanced">Advanced</h3>
					<a name="advanced"></a>
					<p>
						Lorem ipsum sed turpis nibh libero nisi integer arcu mattis <code>quam aliquet</code> aliquet. Viverra aliquam felis lacus mus auctor nisl eu laoreet in.
					</p>

				<!-- DEPENDENCY LIST - BEGINS -->
					{% if model.reference-dependency == "true" %}
						<h6>Dependencies</h6>
						<ul>
							{% if model.reference-dependency-evolution == "true" %}
								<li>
									<a href="/">Evolution Core {{ global.version_number }}</a>
									<ul>
										<li>
											<a href="/reference/base/boxes">Boxes</a>
										</li>
									</ul>
								</li>
							{% endif %}

							{% if model.reference-dependency-foundation == "true" %}
								<li>
									<a href="http://foundation.zurb.com/" target="_blank">Zurb Foundation {{ global.foundation_version }}</a>
								</li>
							{% endif %}

							{% if model.reference-dependency-jquery == "true" %}
								<li><a href="https://jquery.com/" target="_blank">jQuery {{ global.jquery_version }}</a></li>
							{% endif %}

							{% if model.reference-dependency-jquery-ui == "true" %}
								<li><a href="http://jqueryui.com/" target="_blank">jQuery UI {{ global.jquery_ui_version }}</a></li>
							{% endif %}

							{{ model.reference-dependency-list }}
						</ul>
					{% endif %}
				<!-- DEPENDENCY LIST - ENDS -->

				</div>
			</section>
		<!-- ADVANCED - ENDS -->

		</div>
	</section>
<!-- EXAMPLE - ENDS -->

{% endblock %}