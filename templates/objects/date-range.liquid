{% block title %}{{ model.title }} : {{ global.site_title }}{% endblock %}
{% block content %}

<!-- PAGE TITLE - BEGINS -->
	<h1>{{ model.title }}</h1>
<!-- PAGE TITLE - ENDS -->

<!-- CONTENT SECTION - BEGINS -->
	<section class="col-24 mul-alternate-views-badge mul-ref-alternate-views-badge" data-alternate-views-badge="js-mul-ref-alternate-views-badge">

		{% include '/reference/reference-content' %}

	<!-- CONTENT SECTION - CONTENT PANEL - GET CODE - BEGINS -->
		{% if model.reference-get-code == "true" %}
			<div id="mul-ref-alternate-views-badge-content-panel-get-code">

				<h3 class="mul-hdr-rule">HTML</h3>

				{{ model.reference-code-html-content-block-pre }}
				{{ global.reference-get-code-start-tag }}{% include '/objects/date-range' %}{{ global.reference-get-code-end-tag }}
				{{ model.reference-code-html-content-block-post }}

				{% if model.reference-code-js == "true" %}

					<h3 class="mul-hdr-rule">JS</h3>

					{{ model.reference-code-js-content-block-pre }}
					{{ global.reference-get-code-start-tag }}{% include '/js/objects/date-range' %}{{ global.reference-get-code-end-tag }}
					{{ model.reference-code-js-content-block-post }}

				{% endif %}

			</div>
		{% endif %}
	<!-- CONTENT SECTION - CONTENT PANEL - GET CODE - ENDS -->

	</section>
<!-- CONTENT SECTION - ENDS -->

<!-- EXAMPLE SECTION - BEGINS -->
	<section class="col-24 mul-ref-example">

	<!-- EXAMPLE SECTION - HEADER - BEGINS -->
		<h3 class="mul-hdr-dbl-rule mul-ref-example-hdr">{{ global.reference-example-hdr }}</h3>
	<!-- EXAMPLE SECTION - HEADER - ENDS -->

	<!-- EXAMPLE SECTION - CONTENT - BEGINS -->
		<section class="mul-ref-example-content">

			<div class="col-11 first">
				
				<div class="col-24">

				<!-- EXAMPLE SECTION - BOX - BEGINS -->
					<div class="mul-box mul-box-rnd mul-ref-example-box">

						{% if model.reference-example-versions == "true" %}
							<h4 class="mul-hdr-rule">{{ model.reference-example-version-label }}</h4>
						{% endif %}

						<!-- {{ model.title | upcase }} - EXAMPLE - BEGINS -->
							<form class="mul-form" id="" name="" method="" action="" title="">
								<fieldset>
									<ol>
										<li>
											<label class="mul-required" for="js-mul-datepicker-range-stacked-from">
												<i class="mul-icon-mul-asterisk-small mul-icon-color-gray-darker mul-icon-position-left"></i>Label:
											</label>
											{% include '/objects/date-range' %}
										</li>
									</ol>
								</fieldset>
							</form>
						<!-- {{ model.title | upcase }} - EXAMPLE - ENDS -->

					</div>
				<!-- EXAMPLE SECTION - BOX - ENDS -->
					
				</div>
				<div class="col-24">

				<!-- EXAMPLE SECTION - BOX - BEGINS -->
					<div class="mul-box mul-box-rnd mul-ref-example-box">

						{% if model.reference-example-versions == "true" %}
							<h4 class="mul-hdr-rule">{{ model.reference-example-version-label-2 }}</h4>
						{% endif %}

						<!-- {{ model.title | upcase }} - EXAMPLE - BEGINS -->
							<form class="mul-form mul-form-inline" id="" name="" method="" action="" title="">
								<fieldset>
									<ol>
										<li>
											<label class="col-5 first mul-required" for="js-mul-datepicker-range-left-aligned-from">
												<i class="mul-icon-mul-asterisk-small mul-icon-color-gray-darker mul-icon-position-left"></i>Label:
											</label>
											<div class="col-19 mul-datepicker-ctn mul-datepicker-range mul-small-mediaquery-grid">
												<input class="col-12 first" id="js-mul-datepicker-range-left-aligned-from" readonly="true" name="" value="" type="text" tabindex="" title="Select date." placeholder="From" data-mul-datepicker-message-label="This is a calendar message area for user messages related to dates" />
												<input class="col-12" id="js-mul-datepicker-range-left-aligned-to" readonly="true" name="" value="" type="text" tabindex="" title="Select date." placeholder="To" data-mul-datepicker-message-label="This is a calendar message area for user messages related to dates" />
											</div>
										</li>
									</ol>
								</fieldset>
							</form>
						<!-- {{ model.title | upcase }} - EXAMPLE - ENDS -->

					</div>
				<!-- EXAMPLE SECTION - BOX - ENDS -->
					
				</div>
				<div class="col-24">

				<!-- EXAMPLE SECTION - BOX - BEGINS -->
					<div class="mul-box mul-box-rnd mul-ref-example-box">

						{% if model.reference-example-versions == "true" %}
							<h4 class="mul-hdr-rule">{{ model.reference-example-version-label-3 }}</h4>
						{% endif %}

						<!-- {{ model.title | upcase }} - EXAMPLE - BEGINS -->
							<form class="mul-form mul-form-inline mul-form-label-right" id="" name="" method="" action="" title="">
								<fieldset>
									<ol>
										<li>
											<label class="col-5 first mul-required" for="js-mul-datepicker-range-right-aligned-from">
												<i class="mul-icon-mul-asterisk-small mul-icon-color-gray-darker mul-icon-position-left"></i>Label:
											</label>
											<div class="col-19 mul-datepicker-ctn mul-datepicker-range mul-small-mediaquery-grid">
												<input class="col-12 first" id="js-mul-datepicker-range-right-aligned-from" readonly="true" name="" value="" type="text" tabindex="" title="Select date." placeholder="From" data-mul-datepicker-message-label="This is a calendar message area for user messages related to dates" />
												<input class="col-12" id="js-mul-datepicker-range-right-aligned-to" readonly="true" name="" value="" type="text" tabindex="" title="Select date." placeholder="To" data-mul-datepicker-message-label="This is a calendar message area for user messages related to dates" />
											</div>
										</li>
									</ol>
								</fieldset>
							</form>
						<!-- {{ model.title | upcase }} - EXAMPLE - ENDS -->

					</div>
				<!-- EXAMPLE SECTION - BOX - ENDS -->
					
				</div>

			</div>
			<div class="col-13">
				
			</div>

		</section>
	<!-- EXAMPLE SECTION - CONTENT - ENDS -->

	</section>
<!-- EXAMPLE SECTION - ENDS -->

{% endblock %}