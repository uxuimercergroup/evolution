										<div class="col-24 evo-tbl-data-filter">
											<input id="color-vars-filter" type="text" title="Type to filter results." tabindex="" placeholder="Filter" />
										</div>
									</caption>
									<thead>
										<tr>
											<th class="col-16" scope="col">SASS Variable</th>
											<th class="col-8" scope="col">Hex Value</th>
										</tr>
									</thead>
									<tbody>
										
										{% for item in model.color_variables_data %}
										<tr {% if item.default_text_color == "true" %}class="default-text-color"{% endif %} style="background-color: {{ item.hex_value }};">
											<td>{{ item.sass_variable }}</td>
											<td>{{ item.hex_value }}</td>
										</tr>
