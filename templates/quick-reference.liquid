							<h4 class="evo-hdr-rule">Base Variables</h4>
						</div>
						<div class="evo-box-bdy">

							<!-- BASE VARIABLES TABLE - BEGINS -->
							<div class="evo-table-small-mediaquery">
								<table class="evo-table evo-footable evo-table-borders" border="0" cellpadding="0" cellspacing="0">
									<thead>
										<tr>
											<th class="col-8" scope="col">SASS Variable</th>
											<th class="col-8" scope="col">Function</th>
											<th class="col-8" scope="col">Value</th>
										</tr>
									</thead>
									<tbody>
										
										{% for item in model.base_variables_data %}
										<tr>
											<td>{{ item.sass_variable }}</td>
											<td>{{ item.function }}</td>
											<td>{{ item.value }}</td>
										</tr>
										{% endfor %}

									</tbody>
								</table>
							</div>
							<!-- BASE VARIABLES TABLE - ENDS -->
							
						</div>
					</div>
