{% block title %}{{ model.title }} : {{ global.site_title }}{% endblock %}
{% block content %}
<div class="col-24">

	<h1 class="mul-hdr-rule">{{ model.title }}</h1>
	<p>{{ model.introCopy }}</p>

	<div class="col-16 first">	
	<!-- BROWSER GRADING TABLE - BEGINS -->
		<div class="mul-table-small-mediaquery">
			<table class="mul-table mul-table-borders mul-table-striped mul-table-hover" id="" border="0" cellpadding="0" cellspacing="0" title="" summary="">
				<thead>
					<tr>
						<th class="" scope="col">&nbsp;</th>
						<th class="" scope="col">A Grade</th>
						<th class="" scope="col">B Grade</th>
						<th class="" scope="col">F Grade</th>
					</tr>
				</thead>
				<tbody>
					{% for item in model.browser_table_data %}
					<tr>
						<td class="mul-bold">{{ item.browserType }}</td>
						<td class="">{{ item.gradeA }}</td>
						<td class="">{{ item.gradeB }}</td>
						<td class="">{{ item.gradeF }}</td>
					</tr>
					{% endfor %}
				</tbody>
			</table>
		</div>
	<!-- BROWSER GRADING TABLE - ENDS -->

		<h2 class="mul-h2-hdr-background">FAQ's</h2>
		<ul class="mul-data-list mul-data-list-stacked">
			{% for item in model.browser_faq_data %}
			<li>
				<span class="mul-data-list-label">{{ item.question }}</span>
				<span class="mul-data-list-item">{{ item.answer }}</span>
			</li>
			{% endfor %}
		</ul>
	</div>
	<div class="col-8">
		<h5>{{ model.browserKeyHeader }}</h5>
		<ul class="mul-data-list">
			<li>{{ model.gradeA }}</li>
			<li>{{ model.gradeB }}</li>
			<li>{{ model.gradeF }}</li>
		</ul>
	</div>

</div>
{% endblock %}