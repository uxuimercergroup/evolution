<button class="mul-btn mul-btn-medium mul-btn-primary mul-btn-first" id="js-mul-content-modal-example-btn">Open Content Modal</button>

<!-- Modal dialog source element -->
<div id="js-mul-content-modal-example" class="mul-grid mul-content-modal-ctn">

	<div class="js-mul-content-modal-tabs">
		<div class="col-1 first mul-content-modal-util-bar">

			<!-- TABS MAIN NAVBAR -->
			<ul class="mul-content-modal-tabs mul-content-modal-tab-navbar">
				<li><a class="mul-icon-clipboard mul-icon-size-medium" href="#mul-content-modal-tabs-1" title="Details"></a></li>
				<li><a class="mul-icon-files mul-icon-size-medium" href="#mul-content-modal-tabs-2" title="Documents"></a></li>
				<li><a class="mul-icon-mul-task-download mul-icon-size-medium" href="#mul-content-modal-tabs-3" title="Delegate"></a></li>
				<li class="last-child"><a class="mul-icon-mul-task-reassign mul-icon-size-medium" href="#mul-content-modal-tabs-4" title="Reassign"></a></li>
			</ul>

			<!-- TABS UTILITY NAVBAR -->
			<ul class="mul-content-modal-tabs mul-content-modal-tab-util-bar">
				<!-- <li class="mul-content-modal-navbar-active"><a class="mul-icon-print mul-icon-size-medium" href="javascript:;" title="Print"></a></li> -->
				<li><a class="mul-icon-print mul-icon-size-medium" href="javascript:;" title="Print"></a></li>
				<li class="last-child"><a class="mul-icon-bubble mul-icon-size-medium" href="javascript:;" title="Comment"></a></li>
			</ul>

		</div>

		<!-- MAIN CONTENT AREA -->
		<div class="col-23 mul-content-modal-main-content">

			<!-- TAB 1 - BEGINS -->
			<div class="js-mul-content-modal-scroll" id="mul-content-modal-tabs-1">
				<h1>Header 1</h1>
				{{ 3 | lorem_ipsum:'p' }}
			</div>
			<!-- TAB 1 - ENDS -->

			<!-- TAB 2 - BEGINS -->
			<div class="js-mul-content-modal-scroll" id="mul-content-modal-tabs-2">
				<h1>Header 2</h1>
				{{ 3 | lorem_ipsum:'p' }}

				<!-- INTERACTIVE DATA TABLE - EXAMPLE - BEGINS -->
				<table class="mul-table mul-footable mul-table-borders mul-table-striped mul-table-hover" data-filter="#filter" id="" border="0" cellpadding="0" cellspacing="0" title="" summary="">
					<caption>
						<div class="col-19 first">&nbsp;</div>
						<div class="col-5 mul-tbl-data-filter">
							<input id="filter" type="text" placeholder="Filter" tabindex="" title="Type to filter results." />
						</div>
					</caption>
					<thead>
						<tr>
							<th class="col-1" scope="col" data-sort-ignore="true">
								<input type="checkbox" class="js-mul-select-all-checkboxes">
							</th>
							<th class="col-6" scope="col"><a href="javascript:;">Task</a></th>
							<th data-sort-initial="ascending" class="col-2" scope="col"><a href="javascript:;">Type</a></th>
							<th class="col-8" scope="col">
								<a href="javascript:;">Client</a> |
								<a href="javascript:;">Location</a>
							</th>
							<th class="col-4" scope="col"><a href="javascript:;">Assigned To</a></th>
							<th class="col-3 mul-nowrap mul-align-right" scope="col"><a href="javascript:;">Start Date</a></th>
							<th class="col-3 mul-nowrap mul-align-right" scope="col"><a href="javascript:;">End Date</a></th>
						</tr>
					</thead>
					<tbody>

						{% for item in global.table_data %}
						<tr>
							<td>{{ item.checkbox }}</td>
							<td>{{ item.task }}</td>
							<td>{{ item.type_global }}</td>
							<td>
								<a href="javascript:;">{{ item.company }}</a> |
								{{ item.location}}
							</td>
							<td>
								{{ item.abbrev_first_name}} {{ item.last_name}}
							</td>
							<td class="mul-nowrap mul-align-right">{{ item.start_date }}</td>
							<td class="mul-nowrap mul-align-right">{{ item.end_date }}</td>
						</tr>
						{% endfor %}
						
					</tbody>
				</table>
				<!-- INTERACTIVE DATA TABLE - EXAMPLE - ENDS -->

			</div>
			<!-- TAB 2 - ENDS -->

			<!-- TAB 3 - BEGINS -->
			<div class="js-mul-content-modal-scroll" id="mul-content-modal-tabs-3">
				<h1>Header 3</h1>
				{{ 35 | lorem_ipsum:'p' }}
			</div>
			<!-- TAB 3 - ENDS -->

			<!-- TAB 4 - BEGINS -->
			<div class="js-mul-content-modal-scroll" id="mul-content-modal-tabs-4">
				<h1>Header 4</h1>
				{{ 35 | lorem_ipsum:'p' }}
			</div>
			<!-- TAB 4 - ENDS -->

		</div>
	</div>
</div>