<form class="mul-form mul-form-inline" id="" name="" method="" action="" title="">
	<fieldset>
		<ol>
			<li>
				<label class="col-10 first mul-required" for="">
					<i class="mul-icon-mul-asterisk-small mul-icon-color-gray-darker mul-icon-position-left"></i>Basic Text Input:
				</label>
				<input class="col-14" id="" name="" value="" type="text" tabindex="" title="" />
			</li>
			<li>
				<label class="col-10 first mul-required" for="">
					<i class="mul-icon-mul-asterisk-small mul-icon-color-gray-darker mul-icon-position-left"></i>Label:
				</label>
				<select class="col-14" id="" tabindex="" title="">
					<option selected="selected" value="Select an item">choose one&hellip;</option>
					<option value="">One</option>
					<option value="">Two</option>
					<option value="">Three</option>
				</select>
			</li>
			<li>
				<label class="col-10 first mul-required">
					<i class="mul-icon-mul-asterisk-small mul-icon-color-gray-darker mul-icon-position-left"></i>Label Posed as a Question?
				</label>
				<div class="mul-radio col-14">
					<label for="question1">
						<input id="question1" name="question" value="" type="radio" tabindex="" title="" /> Yes
					</label>
					<label for="question2">
						<input id="question2" name="question" value="" type="radio" tabindex="" title="" /> No
					</label>
				</div>
			</li>
			<li>
				<label class="col-10 first mul-required">
					<i class="mul-icon-mul-asterisk-small mul-icon-color-gray-darker mul-icon-position-left"></i>Year of Original Appointment:
				</label>
				<div class="mul-checkbox col-14">
					<label for="2009">
						<input id="2009" name="" value="" type="checkbox" tabindex="" title="" /> 2009
					</label>

					<label for="2010">
						<input id="2010" name="" value="" type="checkbox" tabindex="" title="" /> 2010
					</label>

					<label for="2011">
						<input id="2011" name="" value="" type="checkbox" tabindex="" title="" /> 2011
					</label>

					<label for="2012">
						<input id="2012" name="" value="" type="checkbox" tabindex="" title="" /> 2012
					</label>
				</div>
			</li>
			<li>
				<label class="col-10 first mul-required" for="">
					<i class="mul-icon-mul-asterisk-small mul-icon-color-gray-darker mul-icon-position-left"></i>Comments:
				</label>
				<textarea class="col-14" id="" name="" tabindex="" title=""></textarea>
			</li>
			<li>
				<label class="col-10 first mul-required" for="js-mul-datepicker-range-left-aligned-from">
					<i class="mul-icon-mul-asterisk-small mul-icon-color-gray-darker mul-icon-position-left"></i>Label:
				</label>
				<div class="col-14 mul-datepicker-ctn mul-datepicker-range mul-small-mediaquery-grid">
					<input class="col-12 first" id="js-mul-datepicker-range-left-aligned-from" readonly="true" name="" value="" type="text" tabindex="" title="Select date." placeholder="From" data-mul-datepicker-message-label="This is a calendar message area for user messages related to dates" />
					<input class="col-12" id="js-mul-datepicker-range-left-aligned-to" readonly="true" name="" value="" type="text" tabindex="" title="Select date." placeholder="To" data-mul-datepicker-message-label="This is a calendar message area for user messages related to dates" />
				</div>
				<span class="col-24">
					<small class="offset-10 col-14 mul-info">This is form info text.</small>
				</span>
			</li>
			<li class="mul-form-action">
				<button class="mul-btn mul-btn-medium mul-btn-first" id="" name="" value="" type="button" tabindex="" title="Cancel">Cancel</button>
				<button class="mul-btn mul-btn-medium mul-btn-primary" id="" name="" value="" type="button" tabindex="" title="Save">Save</button>
			</li>
		</ol>
	</fieldset>
</form>
