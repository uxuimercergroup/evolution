<!-- STATIC DATA TABLE - BEGINS -->
<div class="evo-table-small-mediaquery">
	<table class="evo-table" id="" border="0" cellpadding="0" cellspacing="0" title="" summary="" role="grid">
		<thead>
			<tr>
				<th width="200" scope="column">Browser</th>
				<th scope="column">Platform</th>
				<th width="100" scope="column">CSS Grade</th>
			</tr>
		</thead>
		<tbody>{% for item in global.table_data limit: 10 %}
			<tr>
				<td>{{ item.browser }}</td>
				<td>{{ item.platform }}</td>
				<td class="text-right">{{ item.cssGrade }}</td>
			</tr>{% endfor %}			
		</tbody>
	</table>
</div>
<!-- STATIC DATA TABLE - ENDS -->