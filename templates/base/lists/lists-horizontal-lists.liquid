{% block title %}{{ model.title }} : {{ global.site_title }}{% endblock %}
{% block content %}

<!-- PAGE TITLE - BEGINS -->
	<h1>{{ model.title }}</h1>
<!-- PAGE TITLE - ENDS -->

<!-- CONTENT SECTION - BEGINS -->
	<section class="col-24 mul-alternate-views-badge mul-ref-alternate-views-badge" data-alternate-views-badge="js-mul-ref-alternate-views-badge">

		{% include '/reference/reference-content' %}

	<!-- CONTENT SECTION - CONTENT PANEL - GET CODE - BEGINS -->
		{% if model.reference-get-code == "true" %}
			<div id="mul-ref-alternate-views-badge-content-panel-get-code">

				<h3 class="mul-hdr-rule">HTML</h3>

				{{ model.reference-code-html-content-block-pre }}

				{% if model.reference-example-versions == "true" %}
					<h4>{{ model.reference-example-version-label }}</h4>
				{% endif %}
				{{ global.reference-get-code-start-tag }}{% include '/base/lists/list-horizontal-default' %}{{ global.reference-get-code-end-tag }}
				{{ model.reference-code-html-content-block-post }}

				{% if model.reference-example-versions == "true" %}
					<h4>{{ model.reference-example-version-label-2 }}</h4>
				{% endif %}
				{{ model.reference-code-html-content-block-pre-2 }}
				{{ global.reference-get-code-start-tag }}{% include '/base/lists/list-horizontal-default-links' %}{{ global.reference-get-code-end-tag }}
				{{ model.reference-code-html-content-block-post-2 }}

				{% if model.reference-example-versions == "true" %}
					<h4>{{ model.reference-example-version-label-3 }}</h4>
				{% endif %}
				{{ model.reference-code-html-content-block-pre-3 }}
				{{ global.reference-get-code-start-tag }}{% include '/base/lists/list-horizontal-default-pipes' %}{{ global.reference-get-code-end-tag }}
				{{ model.reference-code-html-content-block-post-3 }}

				{% if model.reference-example-versions == "true" %}
					<h4>{{ model.reference-example-version-label-4 }}</h4>
				{% endif %}
				{{ model.reference-code-html-content-block-pre-4 }}
				{{ global.reference-get-code-start-tag }}{% include '/base/lists/list-horizontal-default-links-pipes' %}{{ global.reference-get-code-end-tag }}
				{{ model.reference-code-html-content-block-post-4 }}

				{% if model.reference-example-versions == "true" %}
					<h4>{{ model.reference-example-version-label-5 }}</h4>
				{% endif %}
				{{ model.reference-code-html-content-block-pre-5 }}
				{{ global.reference-get-code-start-tag }}{% include '/base/lists/list-horizontal-definition' %}{{ global.reference-get-code-end-tag }}
				{{ model.reference-code-html-content-block-post-5 }}

				{% if model.reference-example-versions == "true" %}
					<h4>{{ model.reference-example-version-label-6 }}</h4>
				{% endif %}
				{{ model.reference-code-html-content-block-pre-6 }}
				{{ global.reference-get-code-start-tag }}{% include '/base/lists/list-horizontal-definition-links' %}{{ global.reference-get-code-end-tag }}
				{{ model.reference-code-html-content-block-post-6 }}

				{% if model.reference-example-versions == "true" %}
					<h4>{{ model.reference-example-version-label-7 }}</h4>
				{% endif %}
				{{ model.reference-code-html-content-block-pre-7 }}
				{{ global.reference-get-code-start-tag }}{% include '/base/lists/list-horizontal-definition-pipes' %}{{ global.reference-get-code-end-tag }}
				{{ model.reference-code-html-content-block-post-7 }}

				{% if model.reference-code-js == "true" %}

					<h3 class="mul-hdr-rule">JS</h3>

					{% if model.reference-example-versions == "true" %}
						<h4>{{ model.reference-example-version-label }}</h4>
					{% endif %}
					{{ model.reference-code-js-content-block-pre }}
					{{ global.reference-get-code-start-tag }}{ include '/js/' }{{ global.reference-get-code-end-tag }}
					{{ model.reference-code-js-content-block-post }}

				{% endif %}

			</div>
		{% endif %}
	<!-- CONTENT SECTION - CONTENT PANEL - GET CODE - ENDS -->

	</section>
<!-- CONTENT SECTION - ENDS -->

<!-- EXAMPLE SECTION - BEGINS -->
	<section class="col-24 mul-ref-example">

	<!-- EXAMPLE SECTION - HEADER - BEGINS -->
