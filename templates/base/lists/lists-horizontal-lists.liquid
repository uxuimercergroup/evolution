						<!-- {{ model.title | upcase }} - EXAMPLE - ENDS -->

					</div>
				<!-- EXAMPLE SECTION - BOX - ENDS -->
					
				</div>
				<div class="col-8">

				<!-- EXAMPLE SECTION - BOX - BEGINS -->
					<div class="mul-box mul-box-rnd mul-padding-bottom mul-ref-example-box">

						{% if model.reference-example-versions == "true" %}
							<h4 class="mul-hdr-rule">{{ model.reference-example-version-label-6 }}</h4>
						{% endif %}

						<!-- {{ model.title | upcase }} - EXAMPLE - BEGINS -->
							{% include '/base/lists/list-horizontal-definition-links' %}
						<!-- {{ model.title | upcase }} - EXAMPLE - ENDS -->

					</div>
				<!-- EXAMPLE SECTION - BOX - ENDS -->
					
				</div>
				<div class="col-8">

				<!-- EXAMPLE SECTION - BOX - BEGINS -->
					<div class="mul-box mul-box-rnd mul-padding-bottom mul-ref-example-box">

						{% if model.reference-example-versions == "true" %}
							<h4 class="mul-hdr-rule">{{ model.reference-example-version-label-7 }}</h4>
						{% endif %}

						<!-- {{ model.title | upcase }} - EXAMPLE - BEGINS -->
							{% include '/base/lists/list-horizontal-definition-pipes' %}
						<!-- {{ model.title | upcase }} - EXAMPLE - ENDS -->

					</div>
				<!-- EXAMPLE SECTION - BOX - ENDS -->
					
				</div>
			</div>

		</section>
	<!-- EXAMPLE SECTION - CONTENT - ENDS -->

	</section>
<!-- EXAMPLE SECTION - ENDS -->

{% endblock %}