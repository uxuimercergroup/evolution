---
title: "Corporate Logos"
---

<!-- INTRO SECTION - BEGINS -->
	<section class="row">
		<div class="small-12 columns evo-spacer-padding-top-medium evo-site--pattern-library-intro">

{{# markdown }}
## {{ title }}
The corporate logos below are to be used for branding digital products across all lines of business. These logos are designed and optimized for placement in product headers and footers for all global Web properties.
{{/ markdown }}

		{{> button href="#jumpToMenu" expand="true" text="Jump To Nav" custom-class="show-for-small-only tiny"}}

		<!-- TABS - BEGINS -->
			<ul class="button-group radius even-4 stack-for-small evo-site-tabs--pattern-library-tabs" data-tab role="tablist">
				{{#each pattern-library.tabs}}
				<li class="tab-title{{#is active 'true'}} active{{/is}}" role="presentation">
					<a href="#tabs{{@index}}" role="tab" tabindex="0" aria-selected="true" aria-controls="tabs{{@index}}" class="button small evo-button">
						<span class="text">{{name}}</span>
					</a>
				</li>
				{{/each}}
			</ul>
		<!-- TABS - ENDS -->

		</div>
	</section>
<!-- INTRO SECTION - ENDS -->

<!-- TABS - BEGINS -->
	<div class="tabs-content evo-tabs-content evo-site-tabs-content--pattern-library-content">
	<!-- TAB PANEL 1 - BEGINS -->
		<section role="tabpanel" aria-hidden="false" class="content active evo-site-tabs-content--pattern-library-tab-panel" id="tabs0">

			<section class="row" data-equalizer>
				<div class="medium-12 large-9 columns evo-keyline-right evo-spacer-padding-top-large" data-equalizer-watch>
				<!-- EXAMPLE BOX - BEGINS -->
					{{#each corp-logos.data }}
					<div class="panel evo-box evo-site-example-box">
						<div class="evo-box-hdr">
						<!-- EXAMPLE HEADER - BEGINS -->
							<h6 class="evo-hdr-rule" data-magellan-destination="{{hyphenate name}}">{{name}}</h6>
							<a name="{{hyphenate name}}"></a>
						<!-- EXAMPLE HEADER - ENDS -->
						</div>
						<div class="evo-box-bdy">
						<!-- RENDERED HTML - BEGINS -->
							{{#is multiple_example 'true'}}
							<ul class="medium-block-grid-2 large-block-grid-2 evo-site-corp-logo-ctn">
								{{#each multiple_example_options}}
								<li>
									<div class="evo-site-corp-logo-ctn--img-ctn" data-evo-background-color="{{display_logo_background_color}}" data-evo-border-color="{{display_logo_border_color}}"><img src="/assets/images/core/corp-logos/{{image_png}}.png" alt="{{display_logo_alt}}" width="{{display_logo_width}}" class="evo-no-margin" /></div>
									<h6 class="evo-hdr-rule">{{name}}</h6>
									<p>{{{description}}}</p>
									<p class="evo-spacer-margin-bottom-small">Downloads:</p>
									<ul class="inline-list evo-inline-list evo-inline-list-pipes">
										<li>
											<a href="/assets/images/core/corp-logos/{{image_png}}.png">PNG</a>
										</li>
										<li>
											<a href="/assets/images/core/corp-logos/{{image_eps}}.eps">EPS</a>
										</li>
										<li>
											<a href="/assets/docs/core/images/corp-logos/{{image_zip}}.zip">Complete Set</a>
										</li>
									</ul>
								</li>
								{{/each}}
							</ul>
							{{/is}}
						<!-- RENDERED HTML - ENDS -->
						</div>
					</div>
					{{/each}}
				<!-- EXAMPLE BOX - ENDS -->
				</div>
				<div class="large-3 columns show-for-large-up evo-spacer-padding-top-large" data-equalizer-watch>
				<!-- SIDENAV - BEGINS -->
					<div class="evo-site--pattern-library-sidenav-magellan" data-magellan-expedition="fixed">
						<dl class="sub-nav">
							{{#each corp-logos.data}}
							<dd data-magellan-arrival="{{hyphenate name}}"><a href="#{{hyphenate name}}">{{name}}</a></dd>
							{{/each}}
						</dl>
					</div>
				<!-- SIDENAV - ENDS -->
				</div>
			</section>

		</section>
	<!-- TAB PANEL 1 - ENDS -->

	<!-- TAB PANEL 2 - BEGINS -->
		<section role="tabpanel" aria-hidden="true" class="content evo-site-tabs-content--pattern-library-tab-panel" id="tabs1">

			<section class="row" data-equalizer>
				<div class="medium-12 large-9 columns evo-keyline-right" data-equalizer-watch>
					Content for something 2
				</div>
				<div class="large-3 columns show-for-large-up" data-equalizer-watch>
					Magellan 2
				</div>
			</section>
			
		</section>
	<!-- TAB PANEL 2 - BEGINS -->

	<!-- TAB PANEL 3 - BEGINS -->
		<section role="tabpanel" aria-hidden="true" class="content evo-site-tabs-content--pattern-library-tab-panel" id="tabs2">

			<section class="row" data-equalizer>
				<div class="medium-12 large-9 columns evo-keyline-right" data-equalizer-watch>
					Content for something 3
				</div>
				<div class="large-3 columns show-for-large-up" data-equalizer-watch>
					Magellan 3
				</div>
			</section>
			
		</section>
	<!-- TAB PANEL 3 - BEGINS -->

	<!-- TAB PANEL 4 - BEGINS -->
		<section role="tabpanel" aria-hidden="true" class="content evo-site-tabs-content--pattern-library-tab-panel" id="tabs3">

			<section class="row" data-equalizer>
				<div class="medium-12 large-9 columns evo-keyline-right" data-equalizer-watch>
					Content for something 4
				</div>
				<div class="large-3 columns show-for-large-up" data-equalizer-watch>
					Magellan 4
				</div>
			</section>
			
		</section>
	<!-- TAB PANEL 4 - BEGINS -->
	</div>
<!-- TABS - ENDS -->